{
  "name": "zombie-simulation-frontend",
  "version": 2,
  "public": true,
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "build",
  "env": {
    "REACT_APP_API_URL": "https://tu-api-url.vercel.app/api"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://tu-api-backend.vercel.app/api/:path*"
    }
  ]
} 